generator client {
  provider = "prisma-client-js"
  output   = "../../../../node_modules/@prisma/auth-client"
}

datasource db {
  provider = "postgresql"
  url      = env("AUTH_DATABASE_URL")
}

model User {
  id String @id @default(uuid())

  email    String @unique()
  name     String
  password String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
